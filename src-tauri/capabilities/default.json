{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:webview:allow-print",
    "opener:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-create"
  ]
}
